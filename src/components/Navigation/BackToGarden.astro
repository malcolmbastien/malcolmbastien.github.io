---
interface Props {
    class?: string;
}

const { class: className } = Astro.props;
---

<nav class={className}>
    <a
        href="/"
        class="inline-flex items-center gap-2 text-[10px] font-bold uppercase tracking-widest text-slate-400 hover:text-primary dark:hover:text-white transition-colors"
    >
        <span class="material-symbols-outlined text-sm">arrow_back</span>
        Return to Garden
    </a>
</nav>
